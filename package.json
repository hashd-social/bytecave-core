{
  "name": "bytecave-core",
  "version": "1.0.0",
  "description": "ByteCave Core - HTTP and P2P storage node API",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev2": "DATA_DIR=./data2 PORT=3010 NODE_URL=http://localhost:3010 NODE_ID=vault-node-2 yarn dev",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:local": "tsx test-local.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "cauldron": "./scripts/cauldron.sh",
    "cauldron:stop": "./scripts/cauldron-stop.sh"
  },
  "keywords": [
    "bytecave",
    "storage",
    "decentralized",
    "p2p",
    "libp2p"
  ],
  "author": "HASHD Team",
  "license": "MIT",
  "dependencies": {
    "@chainsafe/libp2p-noise": "^17.0.0",
    "@chainsafe/libp2p-yamux": "^8.0.0",
    "@libp2p/bootstrap": "^12.0.0",
    "@libp2p/circuit-relay-v2": "^4.1.0",
    "@libp2p/dcutr": "^4.0.0",
    "@libp2p/floodsub": "^11.0.10",
    "@libp2p/identify": "^4.0.0",
    "@libp2p/kad-dht": "^14.0.0",
    "@libp2p/mdns": "^12.0.0",
    "@libp2p/tcp": "^11.0.0",
    "@libp2p/webrtc": "^6.0.10",
    "@libp2p/websockets": "^10.0.0",
    "@multiformats/multiaddr": "^13.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.1.0",
    "it-pipe": "^3.0.1",
    "libp2p": "^3.1.0",
    "multiformats": "^13.0.0",
    "tweetnacl": "^1.0.3",
    "uint8arrays": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
